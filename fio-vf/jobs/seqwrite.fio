[global]
percentile_list=${percentile_list}
ioengine=${ioengine}
direct=1
thread
group_reporting
name=baseline

[fio_job_configs]
stonewall
name=seqwrite_128k_j${seq_job_cnt}_qd${seq_job_qd}
size=100%
bs=128K
rw=write
numjobs=1
iodepth=128
time_based
ramp_time=${ramp_time}
runtime=${runtime}
log_avg_msec=1000
per_job_logs=0
write_bw_log=${output_name}
write_iops_log=${output_name}
write_lat_log=${output_name}